@model List<Category>
<table class="table table-striped" >
    <thead>
        <tr>
            <th>Category ID</th>
            <th>Category Name</th>
            <th>Category Description</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var i in Model){
        <tr>
            <td>@i.CategoryID</td>
            <td>@i.CategoryName</td>
            <td>@i.Description</td>
        </tr>
        }
    </tbody>

